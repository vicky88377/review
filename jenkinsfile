pipeline
              {
                             agent any
                                           stages
                                           {
                                                                        stage('compile-stage')
                                                                        {
                                                                          steps   
                                                                          {           
                                                                                      withMaven(maven:'Maven_3_3_9')
                                                                                      {
                                                                                                     sh 'mvn -f pom.xml clean compile'
                                                                                      }
                                                                           }          
                                                                        }             
                                                                        stage('Testing-stage')
                                                                        {
                                                                          steps
                                                                          {
                                                                                      withMaven(maven:'Maven_3_3_9')
                                                                                      {
                                                                                                     sh 'mvn -f pom.xml test'
                                                                                      }
                                                                           }          
                                                                        } 
                                                                        
                                                          
                                                                        stage('Build-stage')
                                                                        {
                                                                           steps
                                                                           {
                                                                                      withMaven(maven:'Maven_3_3_9')
                                                                                      {
                                                                                                     sh 'mvn -f pom.xml package'
                                                                                      }
                                                                           } 
                                                                        }  
                             
                                           } 
              }                           

